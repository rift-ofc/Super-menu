-- üéÆ –í–ö–õ–ê–î–ö–ê: –ò–ì–†–û–ö
local PlayerTab = Window:MakeTab({Name = "üéÆ –ò–≥—Ä–æ–∫", Icon = "rbxassetid://7733952501"})

-- –°–∫–æ—Ä–æ—Å—Ç—å
local WalkSpeed = 16
PlayerTab:AddSlider({
    Name = "üöÄ –°–∫–æ—Ä–æ—Å—Ç—å –±–µ–≥–∞",
    Min = 16,
    Max = 500,
    Default = 16,
    Color = Color3.fromRGB(0, 255, 136),
    Increment = 1,
    ValueName = "studs",
    Callback = function(value)
        WalkSpeed = value
        if Humanoid then
            Humanoid.WalkSpeed = value
        end
    end
})

-- –ü—Ä—ã–∂–æ–∫
local JumpPower = 50
PlayerTab:AddSlider({
    Name = "ü¶ò –°–∏–ª–∞ –ø—Ä—ã–∂–∫–∞",
    Min = 50,
    Max = 1000,
    Default = 50,
    Color = Color3.fromRGB(255, 136, 0),
    Increment = 5,
    ValueName = "studs",
    Callback = function(value)
        JumpPower = value
        if Humanoid then
            Humanoid.JumpPower = value
        end
    end
})

-- –ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–µ –ø—Ä—ã–∂–∫–∏
local InfiniteJumpEnabled = false
PlayerTab:AddToggle({
    Name = "‚ôæÔ∏è –ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–µ –ø—Ä—ã–∂–∫–∏",
    Default = false,
    Callback = function(state)
        InfiniteJumpEnabled = state
        if state then
            game:GetService("UserInputService").JumpRequest:Connect(function()
                if InfiniteJumpEnabled and Humanoid then
                    Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end)
        end
    end
})

-- Noclip
local NoclipConnection
PlayerTab:AddToggle({
    Name = "üëª –†–µ–∂–∏–º Noclip",
    Default = false,
    Callback = function(state)
        if state then
            NoclipConnection = EnableNoclip()
        elseif NoclipConnection then
            NoclipConnection:Disconnect()
        end
    end
})

-- üëÅÔ∏è –í–ö–õ–ê–î–ö–ê: –í–ò–ó–£–ê–õ–´
local VisualTab = Window:MakeTab({Name = "üëÅÔ∏è –í–∏–∑—É–∞–ª—ã", Icon = "rbxassetid://7733965305"})

-- ESP
VisualTab:AddToggle({
    Name = "üéØ ESP –∏–≥—Ä–æ–∫–æ–≤",
    Default = false,
    Callback = function(state)
        if state then
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character then
                    CreateESP(player)
                end
            end
            Players.PlayerAdded:Connect(function(player)
                player.CharacterAdded:Connect(function()
                    wait(1)
                    CreateESP(player)
                end)
            end)
        else
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character then
                    local esp = player.Character:FindFirstChild("ULTIMATE_ESP")
                    if esp then esp:Destroy() end
                end
            end
        end
    end
})

-- –Ø—Ä–∫–æ—Å—Ç—å
VisualTab:AddToggle({
    Name = "üí° –ü–æ–ª–Ω–∞—è —è—Ä–∫–æ—Å—Ç—å",
    Default = false,
    Callback = function(state)
        if state then
            game.Lighting.Brightness = 2
            game.Lighting.ClockTime = 14
            game.Lighting.GlobalShadows = false
        else
            game.Lighting.Brightness = 1
            game.Lighting.ClockTime = 14
            game.Lighting.GlobalShadows = true
        end
    end
})

-- –¶–≤–µ—Ç –Ω–µ–±–∞
