-- ü§ñ –í–ö–õ–ê–î–ö–ê: –ê–í–¢–û–ú–ê–¢–ò–ó–ê–¶–ò–Ø
local AutoTab = Window:MakeTab({Name = "ü§ñ –ê–≤—Ç–æ-–§–∞—Ä–º", Icon = "rbxassetid://7733678499"})

-- –ê–≤—Ç–æ-–∫–ª–∏–∫–µ—Ä
AutoTab:AddToggle({
    Name = "üñ±Ô∏è –ê–≤—Ç–æ-–∫–ª–∏–∫–µ—Ä",
    Default = false,
    Callback = function(state)
        getgenv().AutoClick = state
        while getgenv().AutoClick do
            task.wait(0.05)
            mouse1click()
        end
    end
})

-- –ê–≤—Ç–æ-—Ñ–µ—Ä–º (—É–Ω–∏–≤–µ—Ä—Å–∞–ª—å–Ω—ã–π)
AutoTab:AddToggle({
    Name = "üí∞ –ê–≤—Ç–æ-—Å–±–æ—Ä –¥–µ–Ω–µ–≥",
    Default = false,
    Callback = function(state)
        getgenv().AutoFarm = state
        while getgenv().AutoFarm do
            task.wait(0.1)
            pcall(function()
                for _, obj in pairs(game.Workspace:GetChildren()) do
                    if obj.Name:lower():find("coin") or 
                       obj.Name:lower():find("money") or 
                       obj.Name:lower():find("cash") then
                        firetouchinterest(Character.HumanoidRootPart, obj, 0)
                        task.wait()
                        firetouchinterest(Character.HumanoidRootPart, obj, 1)
                    end
                end
            end)
        end
    end
})

