-- –ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏
local UIS = game:GetService("UserInputService")
UIS.InputBegan:Connect(function(input, processed)
    if not processed then
        -- Alt + P = –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
        if input.KeyCode == Enum.KeyCode.P and UIS:IsKeyDown(Enum.KeyCode.LeftAlt) then
            print("=== ULTIMATE HUB ===")
            print("–í–µ—Ä—Å–∏—è: 3.0")
            print("–ò–≥—Ä–æ–∫: " .. LocalPlayer.Name)
            print("Game ID: " .. game.PlaceId)
            print("–ò—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—å: " .. (identifyexecutor and identifyexecutor() or "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π"))
        end
        
        -- F9 = –ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏
        if input.KeyCode == Enum.KeyCode.F9 then
            if Humanoid then
                Humanoid.WalkSpeed = WalkSpeed
                Humanoid.JumpPower = JumpPower
                Library:MakeNotification({
                    Name = "–í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ",
                    Content = "–°–∫–æ—Ä–æ—Å—Ç—å –∏ –ø—Ä—ã–∂–æ–∫ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω—ã",
                    Time = 2
                })
            end
        end
    end
end)

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
LocalPlayer.CharacterAdded:Connect(function(newChar)
    Character = newChar
    Humanoid = newChar:WaitForChild("Humanoid")
    wait(1)
    Humanoid.WalkSpeed = WalkSpeed
    Humanoid.JumpPower = JumpPower
end)

-- –§–∏–Ω–∞–ª—å–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ
wait(1)
Library:MakeNotification({
    Name = "–ì–æ—Ç–æ–≤–æ!",
    Content = "–ú–µ–Ω—é –ø–æ–ª–Ω–æ—Å—Ç—å—é –∑–∞–≥—Ä—É–∂–µ–Ω–æ. –£–¥–∞—á–∏ –≤ –∏–≥—Ä–µ!",
    Time = 3
})

